{
  "name": "pf1-psionics",
  "version": "1.0.0",
  "description": "FoundryVTT module that adds Dreamscarred Press' Psionics system to the Pathfinder 1e system",
  "private": true,
  "type": "module",
  "scripts": {
    "packs:extract": "node ./tools/packs.mjs extract",
    "packs:extract:powers": "node ./tools/packs.mjs extract powers",
    "packs:extract:feats": "node ./tools/packs.mjs extract feats",
    "packs:extract:classes": "node ./tools/packs.mjs extract classes",
    "packs:extract:class-abilities": "node ./tools/packs.mjs extract class-abilities",
    "packs:compile": "node ./tools/packs.mjs compile",
    "scrape:powers": "node ./tools/scrapers/powers-scraper.mjs",
    "scrape:powers:single": "node ./tools/scrapers/powers-scraper.mjs",
    "scrape:feats": "node ./tools/scrapers/feats-scraper.mjs",
    "scrape:feats:single": "node ./tools/scrapers/feats-scraper.mjs",
    "scrape:classes": "node ./tools/scrapers/classes-scraper.mjs",
    "scrape:classes:single": "node ./tools/scrapers/classes-scraper.mjs",
    "lint": "eslint \"scripts/**/*.mjs\"",
    "lint:fix": "eslint \"scripts/**/*.mjs\" --fix"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@foundryvtt/foundryvtt-cli": "^3.0.0",
    "eslint": "^9.0.0",
    "js-yaml": "^4.1.0",
    "listr2": "^9.0.3",
    "picocolors": "^1.1.1",
    "typescript-eslint": "^8.0.0",
    "yargs": "^18.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
